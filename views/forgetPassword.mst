{{> header}}

<div>
   <h1>Altere sua senha</h1>

   <form method="POST">
      <label>
         Nova Senha:
         <input required type="password" name="password" />
      </label>
      <br />
      <label>
         Confirmar Nova Senha:
         <input required type="password" name="password-confirm" />
      </label>
      <br /> <br />
      <label>
         <input type="submit" value="Trocar a Senha" />
      </label>
   </form>
</div>

{{> footer}}
